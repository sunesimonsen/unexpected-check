{
  "name": "unexpected-check",
  "description": "Property based testing plugin for unexpected",
  "author": "Sune Simonsen",
  "license": "MIT",
  "repository": "git://github.com/unexpectedjs/unexpected-check.git",
  "version": "2.1.0",
  "main": "lib/unexpected-check.js",
  "scripts": {
    "changelog": "offline-github-changelog > CHANGELOG.md",
    "generate-site": "generate-site --require ./bootstrap-unexpected-markdown.js",
    "lint": "eslint .",
    "test": "mocha test/**/*.spec.js documentation/**/*.md",
    "travis": "npm run lint && npm test",
    "update-examples": "generate-site --require ./bootstrap-unexpected-markdown.js --update-examples",
    "preversion": "npm run lint && npm run test",
    "postversion": "npm run changelog && git add CHANGELOG.md && git commit --allow-empty -m 'Update changelog' && deploy-site.sh"
  },
  "dependencies": {
    "escodegen": "^1.9.0",
    "esprima": "^4.0.0",
    "estraverse": "^4.2.0",
    "ignore": "^5.0.3",
    "pkg-up": "^2.0.0"
  },
  "peerDependencies": {
    "chance-generators": "^3.0.0",
    "unexpected": "^10.40.0 || ^11.0.0-4"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-preset-env": "^1.7.0",
    "chance-generators": "^3.5.0",
    "eslint": "^5.10.0",
    "eslint-config-pretty-standard": "^2.0.0",
    "eslint-plugin-prettier": "^3.0.0",
    "lodash.escape": "^4.0.1",
    "lodash.unescape": "^4.0.1",
    "mocha": "^5.2.0",
    "offline-github-changelog": "^1.2.0",
    "prettier": "^1.15.3",
    "serve": "^10.1.1",
    "unexpected": "11.0.0-4",
    "unexpected-documentation-site-generator": "^5.0.0",
    "unexpected-markdown": "^1.7.4",
    "unexpected-stream": "^3.0.0"
  },
  "keywords": [
    "property",
    "testing",
    "testing",
    "unexpected"
  ]
}
